{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/public/fonts/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/scss/(.*)",
      "dest": "/public/scss/$1"
    },
    {
      "src": "/pariwisata/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/pariwisata/fonts/(.*)",
      "dest": "/public/fonts/$1"
    },
    {
      "src": "/pariwisata/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/pariwisata/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
